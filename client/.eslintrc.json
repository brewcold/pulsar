{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-essential"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": "off",
    "semi": ["error", "always"],
    "no-unused-vars": "off",
    "no-unused-expressions": "off",
    "no-undef": "off",
    "vue/multi-word-component-names": "off"
  }
}
